<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>python_test</servlet-name>
        <servlet-class>pythonTest.file.ServletAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>python_test</servlet-name>
        <url-pattern>/python_test_servlet_action</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login_file</servlet-name>
        <servlet-class>login.file.ServletAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login_file</servlet-name>
        <url-pattern>/login_file_servlet_action</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>register_email</servlet-name>
        <servlet-class>login.file.SendEmailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register_email</servlet-name>
        <url-pattern>/register_email_servlet_action</url-pattern>
    </servlet-mapping>
    <!--  select begin  -->
    <servlet>
        <servlet-name>select_info</servlet-name>
        <servlet-class>select.file.ServletAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>select_info</servlet-name>
        <url-pattern>/select_info_servlet_action</url-pattern>
    </servlet-mapping>
    <!--  select end  -->
    <!--  process begin  -->
    <servlet>
        <servlet-name>process_info</servlet-name>
        <servlet-class>process.file.ServletAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>process_info</servlet-name>
        <url-pattern>/process_info_servlet_action</url-pattern>
    </servlet-mapping>
    <!--  process end  -->
</web-app>